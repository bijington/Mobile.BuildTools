{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "appConfig": {
    "strategy": "BundleAll"
  },
  "artifactCopy": {
    "disable": true
  },
  "automaticVersioning": {
    "behavior": "PreferBuildNumber",
    "environment": "All",
    "versionOffset": 0
  },
  "images": {
    "directories": [
      "images"
    ]
  },
  "manifests": {
    "token": "$",
    "variablePrefix": "Manifest_",
    "missingTokensAsErrors": false,
    "disable": false
  },
  "releaseNotes": {
    "disable": true
  },
  "projectSecrets": {
    "FormsDemo": {
      "properties": [
        {
          "name": "SampleString",
          "type": "String",
          "isArray": false
        },
        {
          "name": "SampleBool",
          "type": "Bool",
          "isArray": false
        },
        {
          "name": "SampleInt",
          "type": "Int",
          "isArray": false
        },
        {
          "name": "SampleDouble",
          "type": "Double",
          "isArray": false
        },
        {
          "name": "SampleUri",
          "type": "String",
          "isArray": false
        }
      ]
    }
  }
}
